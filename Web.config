    <add name="prismsconn" connectionString="Data Source=www.tsu.edu.ph;Initial Catalog=princetech_solutions;Persist Security Info=True;User ID=WEBLogin;Password=WebLogin!@#123" providerName="System.Data.SqlClient" />
