      </properties>
    </profile>
    <roleManager enabled="true">
       <providers>
          <clear />
          <add connectionStringName="spconn" applicationName="/facultyportal" name="AspNetSqlRoleProvider" type="System.Web.Security.SqlRoleProvider" />
          <add applicationName="/facultyportal" name="AspNetWindowsTokenRoleProvider" type="System.Web.Security.WindowsTokenRoleProvider" />
       </providers>
    </roleManager>
    <identity impersonate="false" />
    
	<webServices>
      <protocols>
